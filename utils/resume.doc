            </tr>
        </table>
        <p class=MsoNormal><span lang=EN-US style='font-size:9.0pt;font-family:"微软雅黑","sans-serif"'>&nbsp;</span></p>
        }


        @if (skils.Count > 0)
        {
        <table class=MsoTable15Grid4Accent1 border=1 cellspacing=0 cellpadding=0 style='border-collapse:collapse;border:none'>
            <tr style='height:15pt'>
                <td valign=top colspan=4 valign=top style='border-left:none;border-right:none;background:#17578B;padding:0cm 5.4pt 0cm 5.4pt;height:15pt'>
                    <p class=MsoNormal>
                        <b>
                            <span style='color:#fff;font-size:15.0pt;'>●</span>
                            <span style='color:white;font-size:10.0pt;font-family:"微软雅黑","sans-serif"'>其它<span style='color:white'></span>
                            </span>
                        </b>
                    </p>                  
                </td>
            </tr>
            <tr>
                <td width=553 valign=top style='width:414.8pt;border:none;padding:0cm 5.4pt 0cm 5.4pt'>
                    @foreach (var skil in skils)
                    {
                        <span lang=EN-US style='font-size:10.0pt;font-family:"微软雅黑","sans-serif"'>@skil.SkillName；</span>
                    }
                </td>
            </tr>
        </table>
        <p class=MsoNormal><span lang=EN-US style='font-size:9.0pt;font-family:"微软雅黑","sans-serif"'>&nbsp;</span></p>
        }
    </div>

</body>

</html>
