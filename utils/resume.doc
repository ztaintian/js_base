        /* Page Definitions */
        @@page WordSection1 {
            size: 595.3pt 841.9pt;
            margin: 72.0pt 50.0pt 72.0pt 50.0pt;
            layout-grid: 15.6pt;
        }

        table {
            border-collapse: collapse;
            table-layout: fixed;
            width: 100%;
        }

        td {
            border: 1px solid #ddd;
            word-break: break-all;
        }

        div.WordSection1 {
            page: WordSection1;
        }

        p.tags {
            float: left;
            margin-right: 0;
            padding: 3px 8px;
            background: #eee;
            border-radius: 5px;
        }
